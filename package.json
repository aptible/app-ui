{
  "name": "cloud-ui",
  "version": "0.0.0",
  "description": "Aptible's dashboard",
  "author": "Eric Bower <eric.bower@aptible.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "format.pretty": "rome format . --write",
    "format.lint": "rome check . --apply-suggested",
    "format": "yarn format.pretty && yarn format.lint",
    "lint.check": "rome ci .",
    "lint.types": "tsc --noEmit",
    "lint": "yarn lint.types && yarn lint.check",
    "test": "yarn lint && vitest run",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "rome format --write",
      "rome check --apply-suggested"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitest/ui": "^0.25.1",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "history": "^5.3.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "qrcode.react": "^3.1.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3",
    "redux": "^4.2.0",
    "redux-batched-actions": "^0.5.0",
    "redux-persist": "^6.0.0",
    "rome": "^10.0.0",
    "saga-query": "^4.1.0",
    "tailwindcss": "^3.2.2",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.1"
  }
}
