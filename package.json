{
  "name": "reploy-ui",
  "version": "1.0.0",
  "description": "Aptible's customer dashboard",
  "author": "Eric Bower <eric.bower@aptible.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "yarn lint.types && yarn lint.eslint && yarn lint.prettier",
    "format": "prettier --write src/*/**.{ts,tsx}",
    "lint.eslint": "eslint --ext .ts,.tsx .",
    "lint.prettier": "prettier -c src/**/*.{ts,tsx}",
    "lint.types": "tsc --noEmit",
    "prepare": "husky install",
    "test": "yarn lint"
  },
  "dependencies": {
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.25.0",
    "@vitejs/plugin-legacy": "^1.4.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-packages": "^1.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "history": "^5.0.0",
    "husky": ">=6",
    "immer": "^9.0.2",
    "lint-staged": ">=10",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "redux": "^4.1.0",
    "redux-batched-actions": "^0.5.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "robodux": "^10.1.2",
    "saga-query": "^0.0.3",
    "typescript": "^4.3.2",
    "vite": "^2.3.4",
    "vite-tsconfig-paths": "^3.3.13"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
