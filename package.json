    "dpdm": "^3.13.0",
    "history": "^5.3.0",
    "jsdom": "^21.1.2",
    "msw": "^1.2.1",
    "postcss": "^8.4.23",
    "qrcode.react": "^3.1.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2",
    "redux": "^4.2.1",
    "redux-batched-actions": "^0.5.0",
    "redux-persist": "^6.0.0",
    "rome": "^12.1.1",
