{
  "name": "cloud-ui",
  "version": "0.0.0",
  "description": "Aptible's dashboard",
  "author": "Eric Bower <eric.bower@aptible.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "fmt": "biome check . --apply-unsafe --verbose",
    "lint.types": "tsc --noEmit",
    "lint.check": "biome ci .",
    "lint.circular": "dpdm --no-tree --no-warning --exit-code circular:1 ./src/app/init.ts",
    "lint": "yarn lint.types && yarn lint.check && yarn lint.circular",
    "test": "yarn lint && vitest run"
  },
  "dependencies": {
    "@biomejs/biome": "^1.2.2",
    "@github/webauthn-json": "^2.1.1",
    "@reduxjs/toolkit": "^1.9.6",
    "@remix-run/web-fetch": "^4.4.1",
    "@sentry/react": "^7.73.0",
    "@stripe/react-stripe-js": "^2.3.1",
    "@stripe/stripe-js": "^2.1.7",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/debug": "^4.1.9",
    "@types/luxon": "^3.3.7",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.9",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "chart.js": "^4.4.0",
    "chartjs-adapter-luxon": "^1.3.1",
    "classnames": "^2.3.2",
    "debug": "^4.3.4",
    "dpdm": "^3.14.0",
    "history": "^5.3.0",
    "jsdom": "^22.1.0",
    "luxon": "^3.4.4",
    "msw": "^1.3.2",
    "postcss": "^8.4.31",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0",
    "redux": "^4.2.1",
    "redux-batched-actions": "^0.5.0",
    "redux-persist": "^6.0.0",
    "starfx": "^0.2.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.10",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
