{
  "name": "app-ui",
  "version": "0.0.0",
  "description": "Aptible's dashboard",
  "author": "Eric Bower <eric.bower@aptible.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "start": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "fmt": "biome check . --write --unsafe --verbose",
    "lint.types": "tsc --noEmit",
    "lint.check": "biome ci .",
    "lint.circular": "dpdm --no-tree --no-warning --exit-code circular:1 ./src/app/init.ts",
    "lint": "yarn lint.types && yarn lint.check && yarn lint.circular",
    "test": "yarn lint && vitest run"
  },
  "dependencies": {
    "@biomejs/biome": "^1.8.3",
    "@floating-ui/react": "^0.26.22",
    "@github/webauthn-json": "^2.1.1",
    "@mintlify/widget-react": "^0.0.11",
    "@remix-run/web-fetch": "^4.4.2",
    "@sentry/react": "^8.26.0",
    "@stripe/react-stripe-js": "^2.8.0",
    "@stripe/stripe-js": "^4.3.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/debug": "^4.1.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "chart.js": "^4.4.3",
    "chartjs-adapter-luxon": "^1.3.1",
    "classnames": "^2.5.1",
    "debug": "^4.3.6",
    "dotenv": "^16.4.5",
    "dpdm": "^3.14.0",
    "history": "^5.3.0",
    "jsdom": "^24.1.1",
    "luxon": "^3.5.0",
    "msw": "^1.3.3",
    "postcss": "^8.4.41",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "starfx": "^0.13.2",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
}
