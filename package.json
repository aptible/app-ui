{
  "name": "cloud-ui",
  "version": "0.0.0",
  "description": "Aptible's dashboard",
  "author": "Eric Bower <eric.bower@aptible.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "format.pretty": "rome format . --write",
    "format.lint": "rome check . --apply",
    "format.unsafe": "rome check . --apply-unsafe",
    "format": "yarn format.pretty && yarn format.lint",
    "lint.check": "rome ci .",
    "lint.types": "tsc --noEmit",
    "lint": "yarn lint.types && yarn lint.check",
    "test": "yarn lint && vitest run"
  },
  "dependencies": {
    "@github/webauthn-json": "^2.1.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/ui": "^0.28.2",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "debug": "^4.3.4",
    "history": "^5.3.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.21",
    "qrcode.react": "^3.1.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.7.0",
    "react-router-dom": "^6.7.0",
    "redux": "^4.2.0",
    "redux-batched-actions": "^0.5.0",
    "redux-persist": "^6.0.0",
    "rome": "^12.0.0",
    "saga-query": "^7.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^5.0.2",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.2"
  }
}
