{
  "name": "app-ui",
  "version": "0.0.0",
  "description": "Aptible's dashboard",
  "author": "Eric Bower <eric.bower@aptible.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "start": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "fmt": "biome check --write --unsafe .",
    "lint.types": "tsc --noEmit",
    "lint.check": "biome ci .",
    "lint.circular": "dpdm --no-tree --no-warning --exit-code circular:1 ./src/app/init.ts",
    "lint": "yarn lint.types && yarn lint.check && yarn lint.circular",
    "test": "yarn lint && vitest run"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@dagrejs/dagre": "^1.1.4",
    "@floating-ui/react": "^0.27.12",
    "@github/webauthn-json": "^2.1.1",
    "@mintlify/widget-react": "^0.0.11",
    "@remix-run/web-fetch": "^4.4.2",
    "@sentry/react": "^8.55.0",
    "@sentry/vite-plugin": "^2.23.0",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^5.10.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/debug": "^4.1.12",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.1",
    "@xyflow/react": "^12.6.4",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.4.9",
    "chartjs-adapter-luxon": "^1.3.1",
    "classnames": "^2.5.1",
    "debug": "^4.4.1",
    "dotenv": "^16.4.7",
    "dpdm": "^3.14.0",
    "history": "^5.3.0",
    "jsdom": "^26.1.0",
    "luxon": "^3.6.1",
    "msw": "^2.2.0",
    "postcss": "^8.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.2",
    "react-router-dom": "^7.6.2",
    "react-use-websocket": "^4.13.0",
    "starfx": "^0.14.4",
    "tailwindcss": "3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.2"
  }
